{
  "name": "soum-web-react-ssr",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/static": "^8.0.4",
    "atom": "^1.4.2",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "cra-template-typescript": "1.2.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "i18next": "^24.2.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.1",
    "pako": "^2.1.0",
    "postcss": "^8.4.49",
    "query-string": "^9.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "react-icons": "^5.4.0",
    "react-lazy-load-image-component": "^1.6.3",
    "react-router-dom": "^7.1.1",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.17",
    "typesense": "^1.8.2"
  },
  "scripts": {
    "start:development": "NODE_ENV=development nodemon --watch ./build node ./build/server.js",
    "start:production": "NODE_ENV=production node ./build/server.js",
    "build:development": "NODE_ENV=development node build.mjs",
    "build:production": "NODE_ENV=production node build.mjs",
    "build:watch": "nodemon --watch . --ext \"ts,tsx,js,jsx,css,json\" --ignore ./build --exec \"npm run build:development\"",
    "dev": "concurrently \"npm run build:watch\" \"npm run start:development\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/pako": "^2.0.3",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@types/react-helmet": "^6.1.11",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "concurrently": "^9.1.2",
    "esbuild": "^0.24.2",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-style-plugin": "^1.6.3",
    "nodemon": "^3.1.9"
  }
}
