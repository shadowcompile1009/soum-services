    "lint:ng": "ng lint",
    "lint:ts": "eslint --ext .ts src/**/*.ts",
    "lint:unused": "ts-unused-exports ./tsconfig.json --excludePathsFromReport=dist --maxIssues=9",
    "prettier:check": "prettier -l './src/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write './src/**/*.{ts,tsx}'",
    "ts-unused-exports": "^8.0.0",
    "size-limit": "^7.0.8"
  },
